{
    "name": "masterix21/laravel-licensing-filament-manager",
    "description": "This is my package laravel-licensing-filament-manager",
    "keywords": [
        "Luca Longo",
        "laravel",
        "laravel-licensing-filament-manager"
    ],
    "homepage": "https://github.com/masterix21/laravel-licensing-filament-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Luca Longo",
            "email": "l.longo@ambita.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3|^8.4",
        "filament/filament": "^4.0",
        "illuminate/contracts": "^11.0||^12.0",
        "masterix21/laravel-licensing": "@dev",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^4.1.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "LucaLongo\\LaravelLicensingFilamentManager\\": "src/",
            "LucaLongo\\LaravelLicensingFilamentManager\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LucaLongo\\LaravelLicensingFilamentManager\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LucaLongo\\LaravelLicensingFilamentManager\\LaravelLicensingFilamentManagerServiceProvider"
            ],
            "aliases": {
                "LaravelLicensingFilamentManager": "LucaLongo\\LaravelLicensingFilamentManager\\Facades\\LaravelLicensingFilamentManager"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
